<div class="within-container">
    <div class="new-dish">
        <a href="/macbro/new">add your own meal plans</a>
    </div>
    
    <div class="search-bar">
        <h3>Search for the perfect meal: </h3>
        <form action="/macbro" method="get" class="search-inputs">
            <input class="inputs" type="text" placeholder="calorie" name="calorie">
            <input class="inputs" type="text" placeholder="protein" name="protein">
            <input class="inputs" type="text" placeholder="carbs" name="carbs">
            <input class="inputs" type="text" placeholder="fat" name="fat">
            <input class="inputs" type="text" placeholder="ingredients" name="ingredients">
            <button class="search-button">search</button>
        </form>
    </div>
    
    <div class="list-1">
        <div class="list-grid">
            <% i = 0 %>
            <% while i < index.length%>
            <div> 
                <div class="grid-list">
                    <a class="list" href="/macbro/<%=dishes[index[i]]['id']%>"><%=dishes[index[i]]['name']%>
                    <button><img src="<%=dishes[index[i]]['dish_img']%>" alt="" class="index-images"></button>
                    </a>
                </div>
            </div>
            <% i += 1%>
            <%end%>
        </div>
    </div>
</div>